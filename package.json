  "author": "KirianCaumes",
  "license": "MIT",
  "scripts": {
    "prepare": "npm run build",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
